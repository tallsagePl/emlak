{
  "name": "emlak-parser",
  "version": "1.0.0",
  "description": "Парсер недвижимости с использованием puppeteer-real-browser",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js list",
    "parse": "node index.js run hepsiemlak",
    "parse:test": "node index.js run hepsiemlak --test",
    "parse:limit": "node index.js run hepsiemlak --limit",
    "parse:all": "node index.js run-all",
    "parse:all-test": "node index.js run-all --test",
    "production": "node index.js production",
    "production:test": "node index.js production --test-run",
    "production:force": "node index.js production-force hepsiemlak",
    "stats": "node index.js stats",
    "list": "node index.js list",
    "help": "node index.js --help"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0",
    "node-cron": "^4.1.1",
    "pg": "^8.16.3",
    "progress": "^2.0.3",
    "puppeteer-real-browser": "^1.3.13",
    "xlsx": "^0.18.5",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0"
  },
  "keywords": [
    "parser",
    "puppeteer",
    "real-estate",
    "scraping",
    "cloudflare-bypass"
  ],
  "author": "Developer",
  "license": "MIT"
}
